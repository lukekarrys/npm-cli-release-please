{
  "changelog-sections": [
    {
      "type": "feat",
      "section": "Features",
      "hidden": false
    },
    {
      "type": "fix",
      "section": "Bug Fixes",
      "hidden": false
    },
    {
      "type": "docs",
      "section": "Documentation",
      "hidden": false
    },
    {
      "type": "deps",
      "section": "Dependencies",
      "hidden": false
    },
    {
      "type": "chore",
      "hidden": true
    }
  ],
  "packages": {
    ".": {
      "package-name": "",
      "prerelease": true
    },
    "pkg1": {},
    "pkg2": {}
  },
  "plugins": [
    "node-workspace",
    "workspace-deps"
  ],
  "exclude-packages-from-root": true,
  "separate-pull-requests": true,
  "group-pull-request-title-pattern": "Release ${version}"
}
